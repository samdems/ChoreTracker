        <script>
      // Function to parse the URL and get the value of the 'm' query parameter
      function getQueryParam() {
        const queryString = window.location.search;
        const urlParams = new URLSearchParams(queryString);
        const mParam = urlParams.get('m');
        return mParam;
      }

      // Function to show the info box and set the value of the 'm' parameter
      function showInfoBox() {
        const infoBox = document.getElementById('infoBox');
        const queryParamValue = document.getElementById('queryParamValue');
        const mParam = getQueryParam();
        
        if (mParam) {
          queryParamValue.textContent = mParam;
          infoBox.style.display = 'block';
        }
      }

      // Call the showInfoBox function when the page loads
      window.onload = showInfoBox;
    </script>
